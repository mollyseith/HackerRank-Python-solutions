import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args) {
        int n,i,b,j;
        Scanner sc=new Scanner(System.in);
        n=sc.nextInt();
        int[] a=new int[n];
        for(i=0;i<n;i++){
            a[i]=sc.nextInt();
        }
        for(j=a.length-1;j>=0;j--){
            System.out.println(a[j]+" ");
        }
    }
}